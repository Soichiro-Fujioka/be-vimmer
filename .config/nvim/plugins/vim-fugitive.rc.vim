nnoremap <leader>Ga :Git add %:p<CR><CR>  "git add
nnoremap <leader>Gc :Gcommit<CR><CR>      "git commit
nnoremap <leader>Gs :Gstatus<CR>          "git status
nnoremap <leader>Gp :Gpush<CR>            "git push
nnoremap <leader>Gd :Gdiff<CR>            "git diff
